{
    "name": "jetee",
    "version": "1.0.0",
    "description": "Web轻量级的渐进式MVVM框架",
    "scripts": {
        "build": "npm run build:main && npm run build:plugin && npm run build:npm",
        "build:main": "webpack --config webpack-config/build.js",
        "build:jui": "webpack --config webpack-config/build.plugin.js --env.pluginname=jui",
        "build:lang": "webpack --config webpack-config/build.plugin.js --env.pluginname=lang",
        "build:less": "node ./helper/copy-less.js",
        "build:router": "webpack --config webpack-config/build.plugin.js --env.pluginname=router",
        "build:stat": "webpack --config webpack-config/build.plugin.js --env.pluginname=stat",
        "build:valid": "webpack --config webpack-config/build.plugin.js --env.pluginname=valid",
        "build:plugin": "npm run build:jui && npm run build:lang && npm run build:less && npm run build:router && npm run build:stat && npm run build:valid",
        "build:npm": "node ./helper/build-npm.js",
        "publish": "npm publish npm/jetee && npm publish npm/jui && npm publish npm/lang && npm publish npm/less && npm publish npm/router && npm publish npm/stat && npm publish npm/valid",
        "dev": "webpack-dev-server --open --config webpack-config/dev.js",
        "start": "webpack-dev-server --open --config webpack-config/dev.js",
        "lint": "eslint src --ext js"
    },
    "author": "theajack",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theajack/jetee.git"
    },
    "keywords": [
        "MVVM框架",
        "类vue",
        "web框架",
        "jetee",
        "js库",
        "theajack"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.6.3",
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.5.1",
        "eslint-config-standard": "^14.1.0",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-html": "^6.0.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-markdown-toc": "^1.1.0",
        "gulp-rename": "^2.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.0.3",
        "husky": "^3.0.9",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "lint-staged": "^9.4.2",
        "mini-css-extract-plugin": "^0.9.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "style-loader": "^1.1.3",
        "url-loader": "^4.0.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2"
    },
    "dependencies": {},
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
